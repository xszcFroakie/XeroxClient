{
  "format_version": "1.10.0",
  "particle_effect": {
    "description": {
      "identifier": "xszc:dashtrail.cubics.t13",
      "basic_render_parameters": {
        "material": "particles_add",
        "texture": "textures/XeroxClient/particles/dashtrail/dashcubics/dashcubic13"
      }
    },
    "curves": {
      "variable.size_curve": {
        "type": "linear",
        "input": "v.particle_age",
        "horizontal_range": "v.particle_lifetime",
        "nodes": [0, 90, 90, 90, 90, 0]
      }
    },
    "components": {
      "minecraft:emitter_rate_instant": {
        "num_particles": "math.round(math.random(0, 1.7) * v.emitter_random_1)"
      },
      "minecraft:emitter_lifetime_looping": {
        "active_time": "math.random(0.45, 0.5)"
      },
      "minecraft:emitter_shape_sphere": {
        "radius": "math.random(0.3, 0.5)",
        "direction": "outwards"
      },
      "minecraft:particle_lifetime_expression": {
        "max_lifetime": "math.random(0.8, 1.2)"
      },
      "minecraft:particle_appearance_billboard": {
        "size": [
          "96 * 0.001f * math.sin(v.size_curve)",
          "96 * 0.001f * math.sin(v.size_curve)"
        ],
        "facing_camera_mode": "direction_y",
        "uv": {
          "texture_width": 96,
          "texture_height": 96,
          "uv": [0, 0],
          "uv_size": [96, 96]
        }
      },
      "minecraft:particle_appearance_tinting": {
        "color": [1, 1, 1, 1]
      }
    }
  }
}
