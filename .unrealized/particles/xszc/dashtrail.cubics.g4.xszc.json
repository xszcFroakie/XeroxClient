{
  "format_version": "1.10.0",
  "particle_effect": {
    "description": {
      "identifier": "xszc:dashtrail.cubics.g4",
      "basic_render_parameters": {
        "material": "particles_add",
        "texture": "textures/XeroxClient/particles/dashtrail/dashcubics/group_dashs/dashcubic4"
      }
    },
    "curves": {
      "variable.size_curve": {
        "type": "linear",
        "input": "v.particle_age",
        "horizontal_range": "v.particle_lifetime",
        "nodes": [0, 90, 90, 90, 90, 90, 0]
      }
    },
    "components": {
      "minecraft:emitter_rate_instant": {
        "num_particles": "math.round(math.random(0, 1.7) * v.emitter_random_4)"
      },
      "minecraft:emitter_lifetime_looping": {
        "active_time": "math.random(0.1, 0.15)"
      },
      "minecraft:emitter_shape_sphere": {
        "radius": 0.3,
        "direction": "outwards"
      },
      "minecraft:particle_lifetime_expression": {
        "max_lifetime": 0.7
      },
      "minecraft:particle_appearance_billboard": {
        "size": [
          "48 * 0.001f * math.sin(v.size_curve)",
          "48 * 0.001f * math.sin(v.size_curve)"
        ],
        "facing_camera_mode": "direction_y",
        "uv": {
          "texture_width": 48,
          "texture_height": 768,
          "flipbook": {
            "base_UV": [0, 0],
            "size_UV": [48, 48],
            "step_UV": [0, 48],
            "max_frame": 16,
            "stretch_to_lifetime": true
          }
        }
      },
      "minecraft:particle_appearance_tinting": {
        "color": [1, 1, 1, 1]
      }
    }
  }
}
