{
  "namespace": "common_buttons",

  "default_color@xerox_lib.ac": {
    "ignored": "$ignored_default_anims",
    "from": "$xeroxclient_button_color_hover",
    "to": "$xeroxclient_button_color_default",
    "duration": 0.4,
    "easing": "out_cubic",
    "reset_event": "('button.xeroxclient.' + $pressed_button_name)"
  },

  "default_alpha@xerox_lib.aa": {
    "ignored": "$ignored_default_anims",
    "from": "$xeroxclient_button_alpha_hover",
    "to": "$xeroxclient_button_alpha_default",
    "duration": 0.4,
    "easing": "out_cubic",
    "reset_event": "('button.xeroxclient.' + $pressed_button_name)"
  },

  "hover_color@xerox_lib.ac": {
    "ignored": "$ignored_hover_anims",
    "from": "$xeroxclient_button_color_default",
    "to": "$xeroxclient_button_color_hover",
    "duration": 0.15,
    "easing": "out_cubic",
    "reset_event": "('button.xeroxclient.' + $pressed_button_name)"
  },

  "hover_alpha@xerox_lib.aa": {
    "ignored": "$ignored_hover_anims",
    "from": "$xeroxclient_button_alpha_default",
    "to": "$xeroxclient_button_alpha_hover",
    "duration": 0.15,
    "easing": "out_cubic",
    "reset_event": "('button.xeroxclient.' + $pressed_button_name)"
  },

