  "tab_out_alpha_animation@play_animation.tab_enter_alpha_animation": {
    "duration": 0.6,
    "from": 1.0,
    "to": -0.25,
    "play_event": "screen.exit_push",
    "end_event": "screen.exit_end"
  },

  "tab_out_alpha_animation_pop@play_animation.tab_enter_alpha_animation": {
    "duration": 0.6,
    "from": 1.0,
    "to": -0.25,
    "play_event": "screen.exit_pop",
    "end_event": "screen.exit_end"
  },

  "tab_enter_alpha_wait@xerox_lib.aa": {
    "duration": "$wait_dura",
    "from": -0.25,
    "to": -0.25,
    "play_event": "screen.entrance_push",
    "end_event": "screen.entrance_end",
    "next": "@play_animation.tab_enter_alpha_animation"
  },

  "tab_enter_alpha_animation@xerox_lib.aa": {
    "easing": "out_cubic",
    "duration": 0.6,
    "from": -0.25,
    "to": 1.0
  },

